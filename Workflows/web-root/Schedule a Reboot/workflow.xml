<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:workflow xmlns:ns2="http://vmware.com/vco/workflow" root-name="item1" object-name="workflow:name=generic" id="5d440d7a-1314-4825-992c-d357b3894ae0" editor-version="2.0" version="0.0.3" api-version="6.0.0" restartMode="1" resumeFromFailedMode="0">
    <display-name>Schedule a Reboot</display-name>
    <position y="50.0" x="100.0"/>
    <input>
        <param name="date" type="Date"/>
        <param name="array_in" type="Array/VC:VirtualMachine"/>
    </input>
    <output/>
    <workflow-item name="item0" type="end" end-mode="0" comparator="0">
        <in-binding/>
        <out-binding/>
        <position y="50.0" x="495.0"/>
    </workflow-item>
    <workflow-item name="item1" out-name="item2" type="task" prototype-id="wait-until-date" content-mode="x" comparator="0">
        <display-name>Wait until date</display-name>
        <script encoded="false">//Auto-generated script 
if (waitDate !== null) { 
	System.waitUntil(waitDate,1000);
}
else  { 
	throw "'waitDate' is NULL"; 
}
</script>
        <in-binding>
            <bind name="waitDate" type="Date" export-name="date">
                <description>Wait date</description>
            </bind>
        </in-binding>
        <out-binding/>
        <description>Wait until date.</description>
        <position y="60.0" x="150.0"/>
    </workflow-item>
    <workflow-item name="item2" out-name="item0" type="foreach" comparator="0">
        <display-name>Reboot guest OS</display-name>
        <in-binding>
            <bind name="vm" type="Array/VC:VirtualMachine" export-name="array_in">
                <description>Virtual machine to reboot</description>
            </bind>
            <bind name="" type="Array/VC:VirtualMachine" export-name="*array_in"/>
        </in-binding>
        <out-binding/>
        <reference id="BD8080808080808080808080808080806CC180800122528313869552e41805bb1" type="Workflow"/>
        <position y="60.0" x="260.0"/>
    </workflow-item>
</ns2:workflow>
